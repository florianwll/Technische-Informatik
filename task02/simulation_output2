#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5862cf7b04c0 .scope module, "testbench" "testbench" 2 3;
 .timescale -9 -12;
v0x5862cf7fd170_0 .var "clk", 0 0;
v0x5862cf7fd240_0 .net "count", 31 0, v0x5862cf7fcf40_0;  1 drivers
v0x5862cf7fd310_0 .var "reset", 0 0;
S_0x5862cf7b0650 .scope module, "counter_instance" "ModuloCounter" 2 9, 3 2 0, S_0x5862cf7b04c0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 32 "count";
P_0x5862cf7b07e0 .param/l "k" 0 3 2, +C4<00000000000000000000000000010000>;
v0x5862cf7eac50_0 .net "clk", 0 0, v0x5862cf7fd170_0;  1 drivers
v0x5862cf7fcf40_0 .var "count", 31 0;
v0x5862cf7fd020_0 .net "reset", 0 0, v0x5862cf7fd310_0;  1 drivers
E_0x5862cf7eb3f0 .event posedge, v0x5862cf7fd020_0, v0x5862cf7eac50_0;
    .scope S_0x5862cf7b0650;
T_0 ;
    %wait E_0x5862cf7eb3f0;
    %load/vec4 v0x5862cf7fd020_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5862cf7fcf40_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5862cf7fcf40_0;
    %cmpi/e 15, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5862cf7fcf40_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5862cf7fcf40_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5862cf7fcf40_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5862cf7b04c0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x5862cf7fd170_0;
    %inv;
    %store/vec4 v0x5862cf7fd170_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5862cf7b04c0;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "modulo_counter.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5862cf7b04c0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5862cf7fd170_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5862cf7fd310_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5862cf7fd310_0, 0, 1;
    %delay 100000, 0;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5862cf7fd310_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5862cf7fd310_0, 0, 1;
    %delay 50000, 0;
    %vpi_call 2 41 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench2.v";
    "moduloK.v";
